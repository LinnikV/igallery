<div id="<%= dom_id photo %>">
  <p>
    <strong>Image:</strong><br>
    <%= link_to image_tag(photo.image.url), category_photo_path(@category, photo) %>
  </p>

  <p>
   <%= button_to "Like", [:upvote, @category, photo], method: :patch, data: {turbo: "false"} %>
   <%= photo.cached_votes_total %>
  </p>

  <p>
    <strong>User:</strong>
    <%= photo.user.email %>
  </p>
 

  <p>
    <strong>Folder:</strong>
    <%= photo.category.title %>
  </p>

</div>
